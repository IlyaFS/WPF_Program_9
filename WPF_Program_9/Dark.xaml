<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">    
    <Style x:Key="TextBoxStyle" TargetType="TextBox">
        <Setter Property="Control.Background" Value="Silver"/>
        <Setter Property="Control.Foreground" Value="GreenYellow"/>
    </Style>        
    <Style x:Key="ToolBarStyle" TargetType="ToolBar">
        <Setter Property="Control.Background" Value="Gray"/>
        <Setter Property="Control.Foreground" Value="White"/>
    </Style>
    <Style x:Key="DockPanelStyle" TargetType="Menu">
        <Setter Property="Control.Background" Value="DimGray"/>       
    </Style>
    <Style x:Key="ToolBarTrayStyle" TargetType="ToolBarTray">
        <Setter Property="Control.Background" Value="Gray"/>
        <Setter Property="Control.Foreground" Value="White"/>
    </Style>   
</ResourceDictionary>